<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="@color/zm_item_highlight_pressed">
    <item android:state_enabled="false">
        <shape>
            <solid android:color="@color/zm_highlight_disabled" />
            <corners
                android:bottomLeftRadius="9dp"
                android:bottomRightRadius="9dp" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/zm_item_highlight_pressed" />
            <corners
                android:bottomLeftRadius="9dp"
                android:bottomRightRadius="9dp" />
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape>
            <solid android:color="@color/zm_v2_svg_setting_option" />
            <corners
                android:bottomLeftRadius="8dp"
                android:bottomRightRadius="8dp" />
        </shape>
    </item>
</ripple>
