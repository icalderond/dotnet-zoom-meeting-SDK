<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:zoom="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <!--
     You must set android:windowSoftInputMode="stateHidden|adjustResize" in the activity
     to make sure ZMKeyboardDetector can work properly.
    -->
    <us.zoom.uicommon.widget.view.ZMKeyboardDetector
        android:id="@+id/keyboardDetector"
        android:importantForAccessibility="no"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <LinearLayout
        android:id="@+id/container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/zm_v2_chat_input_bg"
        android:orientation="vertical" >


        <LinearLayout
            android:id="@+id/panelForAlert"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <ViewStub
                android:id="@+id/subConnectionAlert"
                android:inflatedId="@+id/inflatedConnectionAlert"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"/>

            <us.zoom.uicommon.widget.view.ZMAlertView
                android:id="@+id/panelServerError"
                zoom:zm_type="WARNING"
                zoom:zm_need_cancel="false"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

        </LinearLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1" >
            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">
            <androidx.coordinatorlayout.widget.CoordinatorLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <androidx.swiperefreshlayout.widget.SwipeRefreshLayout
                    android:id="@+id/swipeRefreshLayout"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ViewStub
                        android:id="@+id/subMMThreadsRecyclerView"
                        android:inflatedId="@+id/inflatedMMThreadsRecyclerView"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"/>

                </androidx.swiperefreshlayout.widget.SwipeRefreshLayout>

            </androidx.coordinatorlayout.widget.CoordinatorLayout>

                <androidx.core.widget.NestedScrollView
                    android:visibility="gone"
                    android:id="@+id/pmcGuideUI"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:fillViewport="true">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@color/zm_white"
                        android:gravity="center"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/zm_ic_pmc_guide_ui"
                            tools:ignore="ContentDescription" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/zm_margin_normal"
                            android:text="@string/zm_lbl_pmc_guide_ui_title_540095"
                            android:textColor="@color/zm_v2_pmc_guide_ui_title"
                            android:textSize="@dimen/zm_font_normal_size" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/zm_margin_normal"
                            android:gravity="center"
                            android:paddingStart="@dimen/zm_padding_smallest"
                            android:paddingEnd="@dimen/zm_padding_smallest"
                            android:text="@string/zm_lbl_pmc_guide_ui_content_540095"
                            android:textColor="@color/zm_v2_im_keyboard_panel_unselected"
                            android:textSize="@dimen/zm_font_normal_size" />
                    </LinearLayout>
                </androidx.core.widget.NestedScrollView>
</FrameLayout>
            <LinearLayout
                android:id="@+id/typingLinear"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:gravity="center_vertical"
                android:paddingLeft="14dp"
                android:paddingRight="14dp"
                android:visibility="gone"
                tools:visibility="visible"
                android:layout_alignParentBottom="true"
                android:orientation="horizontal"
                android:background="@color/zm_chat_typing_bg">
                <TextView
                    android:id="@+id/txtTypingName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="13sp"
                    android:maxWidth="200dp"
                    android:textColor="@color/zm_chat_typing_text"
                    android:singleLine="true"
                    android:ellipsize="middle"
                    />
                <TextView
                    android:id="@+id/txtTyping"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="13sp"
                    android:textColor="@color/zm_chat_typing_text"
                    android:singleLine="true"
                    >

                </TextView>
            </LinearLayout>

            <LinearLayout
